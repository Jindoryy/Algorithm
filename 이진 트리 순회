def Pre(idx): # 전위순회
    if idx < len(a):
        print(a[idx], end=' ')
        Pre(idx * 2)
        Pre(idx * 2 + 1)

def In(idx): # 중위순회
    if idx < len(a):
        In(idx * 2)
        print(a[idx], end=' ')
        In(idx * 2 + 1)

def Post(idx): # 후위순회
    if idx < len(a):
        Post(idx * 2)
        Post(idx * 2 + 1)
        print(a[idx], end=' ')

if __name__ == "__main__":
    a = [0] + [i for i in range(1, 7 + 1)]
    Pre(a[1])
    print()
    In(a[1])
    print()
    Post(a[1])
