def DFS(L, sum):
    global min, res

    if (c - sum) >= 0 and (c - sum) < min:
        min = (c - sum)
        res = sum

    if sum > c or L == n:
        return
    else:
        DFS(L+1, sum + a[L])
        DFS(L+1, sum)

if __name__ == "__main__":
    c, n = map(int, input().split())
    a = []
    for _ in range(n):
        a.append(int(input()))
    min = float('inf')
    res = 0
    DFS(0, 0)
    print(res)
    
# 80점 짜리 코드임
