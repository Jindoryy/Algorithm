T = int(input())
result = []

for _ in range(T):
    n, s, e, k = map(int, input().split())
    num = list(map(int, input().split()))
    num_copy = num[s-1:e]
    num_copy.sort()
    result.append(num_copy[k-1])

for i in range(T):
    print(f"#{i+1} {result[i]}")

# 그냥 단순 구현문제 흐름대로 읽어 나가면 이해 됨
# 강사님 정답이랑 코드가 비슷해서 강사님 코드는 생략
#
