import sys
input = sys.stdin.readline

n = int(input())
spec = []
res = []

for _ in range(n):
    x, y = map(int, input().split())
    spec.append((x, y))

for i in range(n):
    rank = 1
    for j in range(n):
        if spec[i][0] < spec[j][0] and spec[i][1] < spec[j][1]:
            rank += 1

    res.append(rank)

print(*res)
