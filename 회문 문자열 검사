n = int(input())

for i in range(n):
    tmp = input().lower()

    for j in range(len(tmp)//2):
        if tmp[j] != tmp[-1-j]:
            print(f"#{i+1} NO")
            break
    else:
        print(f"#{i+1} YES")
        
# for ~ else 구문 확인하기
# tmp == tmp[::-1] 확인하기

n = int(input())

for i in range(n):
    tmp = input().lower()

    if tmp == tmp[::-1]:
        print(f"#{i+1} YES")
    else:
        print(f"#{i+1} NO")
