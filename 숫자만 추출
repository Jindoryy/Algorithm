def isprime(x):
    cnt = 0

    for i in range(1, x+1):
        if x % i == 0:
            cnt += 1
    return cnt

string = input()
res = 0

for x in string:
    if x.isdecimal():
        res = res * 10 + int(x)

print(res)

prime = isprime(res)
print(prime)

# res = res * 10 + int(x) 문법 확인
# isdecimal() 함수 확인 - 숫자면 True 숫자가 아니면 False 반환
